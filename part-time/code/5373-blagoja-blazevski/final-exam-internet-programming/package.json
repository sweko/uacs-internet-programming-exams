{
  "name": "final-exam-internet-programming",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev -p 4200\" \"npm run start:back\"",
    "build": "next build",
    "build:back": "ng build",
